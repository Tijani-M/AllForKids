{% extends 'dashboard.html.twig' %}

{% block body %}
    <div class="x_panel">
        <div class="x_title">
            <h2>creation Jardin</h2>

            <ul class="nav navbar-right ">
                <li>
                    <a href="{{ path('jardins_index') }}">Back to the list</a>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
    </div>

 <div class="x_content dashboard_graph">
    <div id="demo-form2" data-parsley-validate="" class="form-horizontal form-label-left" novalidate="">
    {{ form_start(form) }}

        <div class="form-group">
            {{ form_label(form.nom, null, {
                'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}
            }) }}
            <div class="col-md-6 col-sm-6 col-xs-12">
                {{ form_widget(form.nom, {
                    'attr': {'class': 'form-control col-md-7 col-xs-12'}
                }) }}
            </div>
        </div>


        <div class="form-group">
            {{ form_label(form.tel, null, {
                'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}
            }) }}
            <div class="col-md-6 col-sm-6 col-xs-12">
                {{ form_widget(form.tel, {
                    'attr': {'class': 'form-control col-md-7 col-xs-12'}
                }) }}
            </div>
        </div>


        <div class="form-group">
            {{ form_label(form.adresse, null, {
                'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}
            }) }}
            <div class="col-md-6 col-sm-6 col-xs-12">
                {{ form_widget(form.adresse, {
                    'attr': {'class': 'form-control col-md-7 col-xs-12'}
                }) }}
            </div>
        </div>

        <div class="form-group">
            {{ form_label(form.lien, null, {
                'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}
            }) }}
            <div class="col-md-6 col-sm-6 col-xs-12">
                {{ form_widget(form.lien, {
                    'attr': {'class': 'form-control col-md-7 col-xs-12'}
                }) }}
            </div>
        </div>

        <div class="form-group">
            {{ form_label(form.image, null, {
                'label_attr': {'class': 'control-label col-md-3 col-sm-3 col-xs-12'}
            }) }}

            <div class="col-md-6 col-sm-6 col-xs-12">
                {{ form_widget(form.image) }}
            </div>
        </div>

        <div class="ln_solid"></div>
        <div class="form-group">
            <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3" align="center">
                <input class="btn btn-primary" type="submit" value="Enregister"/>
            </div>
        </div>
        {{ form_end(form) }}
    </div>
 </div>

{% endblock %}
